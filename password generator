#project 3:password generator
import random
import string
print('hello,welecome to password generator!')
length=int(input('\n enter the length of password'))
lower=string.ascii_lowercase
upper=string.ascii_uppercase
num=string.digits
symbols=string.punctuation
combi=lower+upper+num+symbols
temp=random.sample(combi,length)
password="".join(temp)
combi=string.ascii_letters+string.digits+string.punctuation
password="".join(random.sample(combi,length))
print (password)
